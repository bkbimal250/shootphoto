{
  "watch": [
    "*.js",
    "routes/",
    "models/",
    "middleware/",
    "config/",
    "scripts/"
  ],
  "ext": "js,json",
  "ignore": [
    "node_modules/",
    "*.test.js",
    "*.spec.js",
    "logs/",
    "uploads/",
    ".git/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "delay": "1000",
  "verbose": true,
  "colours": true,
  "exec": "node Server.js"
}
